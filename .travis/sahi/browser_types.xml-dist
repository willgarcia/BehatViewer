<browserTypes>
    <browserType>
        <name>phantomjs</name>
        <displayName>PhantomJS</displayName>
        <icon>safari.png</icon>
        <path>/usr/local/bin/phantomjs</path>
        <options>--proxy=localhost:9999 %basedir%/phantomjs.js</options>
        <processName>phantomjs</processName>
        <capacity>100</capacity>
        <force>true</force>
    </browserType>
</browserTypes>
